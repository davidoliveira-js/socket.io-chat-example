AAA;AACA,KA/DA;AAgEA,IAAA,KAhEA,mBAgEA;AACA,MAAA,SAAA,CAAA,SAAA,CAAA,SAAA,CAAA,KAAA,SAAA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,qBAAA;AAEA,KApEA;AAqEA,IAAA,KArEA,mBAqEA;AACA,MAAA,SAAA,CAAA,SAAA,CAAA,SAAA,CAAA,KAAA,SAAA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,iBAAA;AAEA,KAzEA;AA0EA,IAAA,KA1EA,iBA0EA,IA1EA,EA0EA;AACA,cAAA,IAAA;AACA,aAAA,GAAA;AACA,cAAA,KAAA,KAAA,EAAA;AACA,iBAAA,KAAA,GAAA,KAAA;AACA,WAFA,MAEA;AACA,iBAAA,KAAA,GAAA,IAAA;AACA;;AACA;;AACA,aAAA,GAAA;AACA,cAAA,KAAA,KAAA,EAAA;AACA,iBAAA,KAAA,GAAA,KAAA;AACA,WAFA,MAEA;AACA,iBAAA,KAAA,GAAA,IAAA;AACA;;AACA;;AACA,aAAA,GAAA;AACA,cAAA,KAAA,KAAA,EAAA;AACA,iBAAA,KAAA,GAAA,KAAA;AACA,WAFA,MAEA;AACA,iBAAA,KAAA,GAAA,IAAA;AACA;;AACA;AArBA;AAwBA;AAnGA,GAzFA;AA+LA,EAAA,OA/LA,qBA+LA,CACA;AACA;AACA;AAEA;AACA;AACA,GAtMA;AAyMA,EAAA,OAAA,EAAA;AAEA,IAAA,aAFA,yBAEA,IAFA,EAEA;AAEA,WAAA,MAAA,CAAA,GAAA;AACA,WAAA,WAAA,CAAA,KAAA;AACA,WAAA,MAAA,GAAA,KAAA,WAAA,CAAA,QAAA,GAAA,OAAA,CAAA,IAAA,EAAA,IAAA,CAAA;;AAEA,UAAA,IAAA,CAAA,GAAA,CAAA,MAAA,IAAA,MAAA,EAAA;AACA,aAAA,MAAA,CAAA,IAAA,GAAA,WAAA;AACA,aAAA,MAAA,CAAA,EAAA;;AACA,YAAA,IAAA,CAAA,GAAA,CAAA,QAAA,EAAA;AACA,eAAA,SAAA,CAAA,IAAA,CAAA,iBAAA,IAAA,CAAA,GAAA,CAAA,IAAA,GAAA,KAAA,GAAA,IAAA,CAAA,GAAA,CAAA,QAAA,GAAA,IAAA,CAAA,GAAA,CAAA,IAAA,GAAA,OAAA;AACA,SAFA,MAEA;AACA,eAAA,SAAA,CAAA,IAAA,CAAA,iBAAA,IAAA,CAAA,GAAA,CAAA,IAAA,GAAA,KAAA,GAAA,IAAA,CAAA,GAAA,CAAA,IAAA,GAAA,OAAA;AACA;AACA,OARA,MAQA,IAAA,IAAA,CAAA,GAAA,CAAA,MAAA,IAAA,KAAA,EAAA;AACA,aAAA,MAAA,CAAA,IAAA,GAAA,YAAA;AACA,aAAA,MAAA,CAAA,EAAA;;AACA,YAAA,IAAA,CAAA,GAAA,CAAA,QAAA,EAAA;AACA,eAAA,QAAA,CAAA,IAAA,CAAA,kBAAA,IAAA,CAAA,GAAA,CAAA,IAAA,GAAA,KAAA,GAAA,IAAA,CAAA,GAAA,CAAA,QAAA,GAAA,OAAA;AACA,SAFA,MAEA;AACA,eAAA,QAAA,CAAA,IAAA,CAAA,kBAAA,IAAA,CAAA,GAAA,CAAA,IAAA,GAAA,OAAA;AACA;AACA,OARA,MAQA;AACA,aAAA,MAAA,CAAA,IAAA,GAAA,OAAA;AACA,aAAA,MAAA,CAAA,GAAA;;AACA,YAAA,IAAA,CAAA,GAAA,CAAA,QAAA,EAAA;AACA,eAAA,SAAA,CAAA,IAAA,CAAA,aAAA,IAAA,CAAA,GAAA,CAAA,IAAA,GAAA,KAAA,GAAA,IAAA,CAAA,GAAA,CAAA,QAAA,GAAA,OAAA;AACA,SAFA,MAEA;AACA,eAAA,SAAA,CAAA,IAAA,mBAAA,IAAA,CAAA,GAAA,CAAA,IAAA;AACA;AACA;AAGA;AAnCA;AAzMA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <header class=\"header\">\r\n      <span class=\"noutil\"></span>\r\n      <h1> Ruby Checkers </h1>\r\n      <h3> {{ NameChecker }}</h3>\r\n      <h6> tempo restante: {{ Exp }}</h6>\r\n      <a v-if=\"admin\" :href=\"adminurl\"> ADMIN </a>\r\n    </header>\r\n    <div class=\"container-1\">\r\n      <div class=\"container-2\">\r\n        <div id=\"textarea\" class=\"card\">\r\n          <textarea rows=\"10\" cols=\"24\" placeholder=\"email:senha\" v-model=\"combos\"></textarea>\r\n          <div id=\"stts\">\r\n            <i style=\"color:#de00ff\" class=\"fas fa-info-circle\"></i>\r\n            <span> status:</span>\r\n            <span id=\"estado\">  {{ Engine.estd }}</span>\r\n          </div>\r\n        </div>\r\n        <!-- <div id=\"progress\">\r\n                                          <progress class=\"\" :value=\"tst\" :max=\"crgds\"></progress>\r\n                                        </div>-->\r\n\r\n        <div id=\"engine\" class=\"card\">\r\n          <div id=\"load\" class=\"card  engine-item\">\r\n            <i style=\"color:#2c6afedb;\" class=\"fas fa-battery-full\"></i>\r\n            <span> Carregadas </span>\r\n            <span id=\"num-load\"> {{ Engine.crgds }} </span>\r\n          </div>\r\n          <div id=\"tests\" class=\"card  engine-item\">\r\n            <i style=\"color:#09e590;\" class=\"fas fa-wrench\"></i>\r\n            <span> Testadas </span>\r\n            <span id=\"num-tests\">  {{ Engine.tst }} </span>\r\n          </div>\r\n          <div id=\"lives\" class=\" engine-item card\">\r\n            <i style=\"color:#0cfa00;\" class=\"fas fa-check-circle\"></i>\r\n            <span> Aprovadas </span>\r\n            <span id=\"num-lives\">  {{ Engine.ap }} </span>\r\n          </div>\r\n          <div id=\"dies\" class=\"card  engine-item\">\r\n            <i style=\"color:#f62323;\" class=\"fas fa-times-circle\"></i>\r\n            <span> Reprovadas </span>\r\n            <span id=\"num-dies\">  {{ Engine.rp }} </span>\r\n          </div>\r\n          <div id=\"errors\" class=\"card  engine-item\">\r\n            <i style=\"color:#f8ff00;\" class=\"fas fa-exclamation-circle\"></i>\r\n            <span> Erros </span>\r\n            <span id=\"num-erros\">  {{ Engine.err }} </span>\r\n          </div>\r\n\r\n\r\n        </div>\r\n        <div id=\"btns\">\r\n          <button @click=\"start()\" class=\"button start is-success\">\r\n            <i class=\"fas fa-play\"></i>\r\n            <span> Start </span>\r\n          </button>\r\n          <button @click=\"pause\" class=\"button pause is-warning\">\r\n            <i class=\"fas fa-pause\"></i>\r\n            <span> Pause </span>\r\n          </button>\r\n          <button @click=\"clearT\" class=\"button clear is-danger\">\r\n            <i class=\"fas fa-trash\"></i>\r\n            <span> Clear </span>\r\n          </button>\r\n        </div>\r\n        <div id=\"insert\">\r\n\r\n          <button @click=\"SHOWI('0')\" class=\"button\">\r\n            <i style=\"color:#0cfa00\" class=\"fas fa-check\"></i>\r\n            <span> Aprovadas </span>\r\n            <span @click=\"copyL\"><i style=\"color: var(--text-full)\" class=\"fas fa-copy\"></i></span>\r\n          </button>\r\n          <div class=\"inserts\">\r\n            <textarea class=\"texti\" v-if=\"showA\" disabled v-model=\"livesList\" id=\"insertlive\"></textarea>\r\n          </div>\r\n\r\n          <button @click=\"SHOWI('2')\" class=\"button\">\r\n            <i style=\"color:#f62323;\" class=\"fas fa-times\"></i>\r\n            <span> Reprovadas </span>\r\n            <span @click=\"clearDie\"><i style=\"color:#f62323;\" class=\"fas fa-trash-alt\"></i></span>\r\n          </button>\r\n          <div class=\"inserts\">\r\n            <textarea class=\"texti\" v-if=\"showD\" disabled v-model=\"diesList\" id=\"insertdie\">\r\n            </textarea>\r\n          </div>\r\n\r\n          <button @click=\"SHOWI('1')\" class=\"button\">\r\n            <i style=\"color:#f8ff00;\" class=\"fas fa-exclamation\"></i>\r\n            <span> Erros </span>\r\n            <span @click=\"copyE\"><i style=\"color:var(--text-full);\" class=\"fas fa-copy\"></i></span>\r\n          </button>\r\n          <div class=\"inserts\">\r\n            <textarea class=\"texti\" v-if=\"showE\" disabled v-model=\"errosList\" id=\"inserterros\"></textarea>\r\n          </div>\r\n\r\n          <span class=\"noutil\"></span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  /*var timeout;\r\n\r\n  function warning() {\r\n    timeout = setTimeout(function() {\r\n      this.pause()\r\n    }, 1000);\r\n    return \"\";\r\n  }\r\n\r\n  function noTimeout() {\r\n    clearTimeout(timeout);\r\n  }\r\n\r\n  window.onbeforeunload = warning;\r\n  window.unload = noTimeout;*/\r\n\r\n  import axios from 'axios'\r\n  //\timport { parseISO } from 'date-fns';\r\n  //\timport { zonedTimeToUtc } from 'date-fns-tz';\r\n\r\n  export default {\r\n    components: {},\r\n    data() {\r\n      return {\r\n        nameC: '',\r\n        NameChecker: '',\r\n        Exp: '',\r\n        Time: undefined,\r\n        ERROS: [],\r\n        Engine: {\r\n          ap: 0,\r\n          rp: 0,\r\n          tst: 0,\r\n          err: 0,\r\n          crgds: 0,\r\n          estd: 'Aguardando a lista!'\r\n        },\r\n        combos: '',\r\n        list: [],\r\n        subList: [],\r\n        preventList: [],\r\n        totalList: [],\r\n        count: 0,\r\n        diesList: [],\r\n        livesList: [],\r\n        errosList: [],\r\n        showA: true,\r\n        showE: false,\r\n        showD: false,\r\n        admin: false,\r\n        adminurl: '',\r\n        datexp: Date,\r\n        idc: [],\r\n        userid: '',\r\n        TotalCheckers: []\r\n\r\n      }\r\n    },\r\n    async created() {\r\n      var Id = this.$route.params.id\r\n      this.userid = Id\r\n        this.$socket.emit('user', {\r\n        userid: this.userid\r\n      })\r\n      let Checker = []\r\n\r\n\r\n      await axios.get('http://localhost:7001/users/'+Id).then(res => {\r\n        console.log(res.data)\r\n        if (!res.data.success) {\r\n          window.location.replace('/')\r\n        } else {\r\n          let {\r\n            role,\r\n            expire,\r\n            checker\r\n          } = res.data.response\r\n          this.datexp = expire\r\n          Checker = checker\r\n          if (role == 1) {\r\n            this.admin = true\r\n            this.adminurl = `/bangobalango/${Id}`\r\n          }\r\n        }\r\n      }).catch(err => {\r\n        alert(err)\r\n        window.location.replace('/')\r\n        this.ERROS = err\r\n      })\r\n      for (var i = 0; i < Checker.length; i++) {\r\n        \r\n        await axios.get('http://localhost:7001/checkers/'+Checker[i]).then(res => {\r\n          if (!res.data.success) {\r\n         //   window.reload(0)\r\n          } else {\r\n            let {\r\n              name,\r\n              slug\r\n            } = res.data.response\r\n\r\n            this.TotalCheckers = {name,slug}[i]\r\n          }\r\n        }).catch(err => {\r\n          alert(err)\r\n          this.ERROS = err\r\n        })\r\n      }\r\n\r\n    },\r\n    methods: {\r\n\r\n      async start() {\r\n\r\n        if (this.combos != '' && this.combos != ' ' && this.combos.length > 0) {\r\n          this.Engine.estd = 'Iniciando os testes...'\r\n          this.list = this.combos.split('\\n')\r\n          this.preventList = this.combos.split('\\n')\r\n\r\n          this.Engine.crgds = this.list.length\r\n\r\n          for (var i = 0; i < this.list.length; i++) {\r\n            this.subList.push(this.list[i])\r\n            this.count += 1\r\n            if (this.count == 5) {\r\n              this.totalList.push(this.subList)\r\n              this.subList = []\r\n              this.count = 0\r\n            }\r\n\r\n          }\r\n\r\n          if (this.subList.length > 0) {\r\n            this.totalList.push(this.subList)\r\n          }\r\n\r\n          for (i = 0; i < this.totalList.length; i++) {\r\n            if (i == this.totalList.length - 1) {\r\n              this.$socket.emit('sendCombos', {\r\n                combos: this.totalList[i], flag: false, checkerName: this.nameC\r\n              })\r\n            } else {\r\n              this.$socket.emit('sendCombos', {\r\n                combos: this.totalList[i], flag: true, checkerName: this.nameC\r\n              })\r\n            }\r\n          }\r\n          this.totalList = []\r\n          this.list = []\r\n          this.subList = []\r\n\r\n        } else {\r\n          this.Engine.estd = 'Carregue a lista!'\r\n        }\r\n      },\r\n\r\n\r\n      pause() {\r\n        this.$socket.emit('break', {})\r\n        this.combos = this.preventList\r\n        this.Engine.estd = 'checker pausado!'\r\n      },\r\n\r\n\r\n\r\n      clearDie() {\r\n        this.diesList = ''\r\n        this.Engine.estd = 'Reprovadas apagadas!'\r\n\r\n      },\r\n      clearT() {\r\n        this.combos = ''\r\n        this.Engine.estd = 'Lista excluida!'\r\n      },\r\n      copyL() {\r\n        navigator.clipboard.writeText(this.livesList)\r\n        this.Engine.estd = 'Aprovadas copiadas!'\r\n\r\n      },\r\n      copyE() {\r\n        navigator.clipboard.writeText(this.errosList)\r\n        this.Engine.estd = 'Erros copiados!'\r\n\r\n      },\r\n      SHOWI (info) {\r\n        switch (info) {\r\n          case '0':\r\n            if (this.showA) {\r\n              this.showA = false\r\n            } else {\r\n              this.showA = true\r\n            }\r\n            break;\r\n          case '1':\r\n            if (this.showE) {\r\n              this.showE = false\r\n            } else {\r\n              this.showE = true\r\n            }\r\n            break;\r\n          case '2':\r\n            if (this.showD) {\r\n         