CA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Admin.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <Sidebar />\r\n    <div id=\"header\">\r\n      <span class=\"noutil\"></span>\r\n      <h1> Painel Admin </h1>\r\n    </div>\r\n    <div class=\"container-1\">\r\n      <div class=\"container-2\">\r\n\r\n        <div class=\"card c-table\">\r\n          <div class=\"card-header\">\r\n            <div style=\"color:var(--text-full)\" class=\"card-header-title is-centered\">\r\n              Tabela de usuários\r\n            </div>\r\n          </div>\r\n          <div class=\"card-content\">\r\n            <Table :type=\"'usuario'\" :config=\"configTableUser\" :tableData=\"computedTableUser\" />\r\n          </div>\r\n          <div id=\"card-footer-t\" class=\"card-footer\">\r\n            <Pagination class=\"card-footer-item\" :total=\"filterUser.length\" :perPageOptions=\"paginationUser.perPage\" v-model=\"paginationUser\" />\r\n          </div>\r\n        </div>\r\n        <div class=\"card cad\">\r\n          <div class=\"card-header\">\r\n            <div style=\"color:var(--text-full)\" class=\"card-header-title is-centered\">\r\n              Cadastro de usuários\r\n            </div>\r\n          </div>\r\n          <div class=\"card-content\">\r\n            <div class=\"container-form\">\r\n              <label class=\"label\"> Usuário</label>\r\n              <input type=\"text\" v-model=\"username\" />\r\n              <label class=\"label\">Senha</label>\r\n              <input type=\"password\" v-model=\"password\" />\r\n              <label class=\"label\"> Data de expiração </label>\r\n              <VueSelect\r\n                class=\"multiselect\"\r\n                label=\"name\"\r\n                :reduce=\"name => name.id\"\r\n                :options=\"DatesExp\"\r\n                v-model=\"expiration\"\r\n                placeholder=\"Selecione a data\">\r\n\r\n              </VueSelect>\r\n              <label class=\"label\"> Checker </label>\r\n              <VueSelect\r\n                multiple\r\n                class=\"multiselect\"\r\n                label=\"name\"\r\n                :reduce=\"name => name._id\"\r\n                :options=\"checkers\"\r\n                v-model=\"checker\"\r\n                placeholder=\"Selecione o checker\">\r\n\r\n              </VueSelect>\r\n            </div>\r\n          </div>\r\n          <div class=\"card-footer\">\r\n            <button @click=\"Register\"> Cadastrar </button>\r\n          </div>\r\n        </div>\r\n        <div class=\"card cdc\">\r\n          <div class=\"card-header\">\r\n            <div style=\"color:var(--text-full)\" class=\"card-header-title is-centered\">\r\n              Cadastro de Checker\r\n            </div>\r\n          </div>\r\n          <div class=\"card-content\">\r\n            <div style=\"color:var(--text-full)\" class=\"container-form\">\r\n              <label class=\"label\"> Nome </label>\r\n              <input class=\"input is-small\" type=\"text\" v-model=\"chk.name\" />\r\n              <label class=\"label\"> Slug </label>\r\n              <input class=\"input is-small\" type=\"text\" v-model=\"chk.slug\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"card-footer\">\r\n            <button @click=\"RegisterChk\"> Cadastrar </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n\r\n  import req from '@/services/Api'\r\n  import {\r\n    formatDistanceToNowStrict,\r\n    // formatDistance\r\n    formatRelative\r\n    // format\r\n  } from 'date-fns'\r\n  import VueSelect from 'vue-select'\r\n  import Table from '@/components/Table'\r\n  import Pagination from '@/components/Pagination'\r\n  import Sidebar from '@/components/Sidebar'\r\n  import \"@/assets/css/select.css\";\r\n\r\n  //  import API from '@/services/Api'\r\n\r\n\r\n  export default {\r\n    components: {\r\n      Table,\r\n      Pagination,\r\n      VueSelect,\r\n      Sidebar\r\n    },\r\n    data() {\r\n      return {\r\n        username: '',\r\n        password: '',\r\n        expiration: '',\r\n        DatesExp: [{\r\n          id: 0,\r\n          name: '1 dia'\r\n        },\r\n          {\r\n            id: 1,\r\n            name: '2 dias'\r\n          },\r\n          {\r\n            id: 2,\r\n            name: '3 dias'\r\n          },\r\n          {\r\n            id: 3,\r\n            name: '7 dias'\r\n          },\r\n        ],\r\n        checker: [],\r\n        id: 0,\r\n        chk: {\r\n          name: '',\r\n          slug: ''\r\n        },\r\n        msgs: {\r\n          SuccessUser: false,\r\n          SuccessChk: false,\r\n          SuccessUchk: false,\r\n          ccampNull: false\r\n        },\r\n        ERROS: [],\r\n        checkers: [],\r\n        configTableUser: [{\r\n          nome: 'username',\r\n          slug: 'Nome'\r\n        },\r\n          {\r\n            nome: 'isOn',\r\n            slug: 'Status'\r\n          },\r\n          {\r\n            nome: 'checkerOn',\r\n            slug: 'Checker'\r\n          },\r\n          {\r\n            nome: 'expire',\r\n            slug: 'Expira Em'\r\n          },\r\n          {\r\n            nome: 'updatedAt',\r\n            slug: 'visto por Utimo'\r\n          },\r\n        ],\r\n        paginationUser: {\r\n          page: 1,\r\n          perPage: 10\r\n        },\r\n        userTableData: [],\r\n        filterUser: [],\r\n        searchUser: '',\r\n        chks:[],\r\n        idsC:[]\r\n      }\r\n    },\r\n    methods: {\r\n      async Register() {\r\n\r\n        let name = this.username\r\n        let pass = this.password\r\n        let exp = this.expiration\r\n        let chk = this.checker\r\n\r\n        name = name.replace(/ /g, '')\r\n        pass = pass.replace(/ /g, '')\r\n\r\n        if (!name && !pass && !exp && !chk) {\r\n          alert('preencha todos os campos')\r\n        } else {\r\n          switch (exp.id) {\r\n            case '0':\r\n              var date = new Date()\r\n              var Exp = date.setDate(date.getDate() +1)\r\n              break;\r\n            case '1':\r\n              date = new Date()\r\n              Exp = date.setDate(date.getDate() +2)\r\n              break;\r\n            case '2':\r\n              date = new Date()\r\n              Exp = date.setDate(date.getDate() +3)\r\n              break;\r\n            case '3':\r\n              date = new Date()\r\n              Exp = date.setDate(date.getDate() +7)\r\n              break;\r\n            default:\r\n              date = new Date()\r\n              Exp = date.setDate(date.getDate() +1)\r\n              break;\r\n          }\r\n          \r\n          req.post('users', {\r\n            username: name,\r\n            password: pass,\r\n            expire: Exp,\r\n            checker: chk,\r\n          }, {withCredentials: true}).then(res => {\r\n            if (res.data.success) {\r\n              alert('Usuário cadastrado')\r\n            } else {\r\n              alert(' fail')\r\n            }\r\n          }).catch(err => {\r\n            console.log(err)\r\n            this.ERROS = err\r\n            alert('erro')\r\n          })\r\n        }\r\n\r\n      },\r\n\r\n      async RegisterChk() {\r\n        let name = this.chk.name\r\n        let slug = this.chk.slug\r\n\r\n        name = name.replace(/ /g,\r\n          '')\r\n\r\n        if (!name || !slug) {\r\n          alert('preencha todos os campos')\r\n        } else {\r\n          req.post('checkers', {\r\n            name: name,\r\n            slug: slug,\r\n          }, {withCredentials: true}).then(res => {\r\n            if (res.data.success) {\r\n              alert('checker cadastrado')\r\n            } else {\r\n              alert('fail')\r\n            }\r\n          }).catch(err => {\r\n            console.log(err)\r\n            this.ERROS = err\r\n            alert('erro')\r\n          })\r\n        }\r\n\r\n      },\r\n      SearchUser: function() {\r\n        this.filterUser = this.userTableData\r\n        if (this.searchUser == '' || this.searchUser == ' ') {\r\n          this.filterUser = this.userTableData\r\n        } else {\r\n          this.filterUser = this.userTableData.filter(user => user.nome == this.searchUser)\r\n        }\r\n      },\r\n    },\r\n    async created() {\r\n      this.id = this.$route.params.tk\r\n      await this.$socket.emit('user', {\r\n        userid: this.id\r\n      })\r\n\r\n      await req.get(`users/${this.id}`).then(res => {\r\n        let {\r\n          role,\r\n          checker\r\n        } = res.data.response\r\n        this.idsChks = checker\r\n        // alert(this.idsChks)\r\n\r\n        if (role != 1) {\r\n          window.location.replace('/')\r\n        }\r\n\r\n      }).catch(err => {\r\n        this.ERROS = err\r\n        window.location.replace('/')\r\n      })\r\n\r\n      await req.get(`users/${this.id}`).then(res=> {\r\n        let {\r\n          checker\r\n        } = res.data.response\r\n        this.idsC = checker\r\n      }).catch(err=> {\r\n        this.ERROS = err\r\n      })\r\n\r\n      await req.get(`checkers/`).then(res => {\r\n        this.checkers = res.data.info\r\n      }).catch(err => {\r\n        this.ERROS = err\r\n        //window.location.replace('/')\r\n      })\r\n      await req.get(`users`).then(res => {\r\n        this.userTableData = res.data.result\r\n        this.filterUser = res.data.result\r\n\r\n\r\n        let tst = []\r\n        this.userTableData.forEach(user => {\r\n\r\n          let vpu = formatRelative(new Date(user.updatedAt), new Date()).toString()\r\n          user.updatedAt = vpu\r\n          if (user.isOn) {\r\n            user.isOn = 'online'\r\n          } else {\r\n            user.isOn = 'offline'\r\n          }\r\n\r\n          let r = formatDistanceToNowStrict(new Date(user.expire), {\r\n            unit: 'second',\r\n            addSuffix: true\r\n          })\r\n          if (r.includes('in')) {\r\n            let secs = r.split(' ')[1].toString()\r\n            let date = new Date(null)\r\n            date.setSeconds(secs)\r\n            date = date.toISOString()\r\n            date = date.toString()\r\n            date = date.split('T')\r\n            let sone = date[0].split('-')\r\n            let stwo = date[1].split(':')\r\n            let d = sone[2]\r\n            let h = stwo[0]\r\n            let m = stwo[1]\r\n            let s = stwo[2].slice(0, 2)\r\n            let z = `${d-1}d:${h}h:${m}m:${s}s`\r\n            user.expire = z\r\n          } else {\r\n            req.delete('users/'+user._id).then(del => {\r\n              if (del.data.success) {\r\n                alert(`usuário ${user.username} Deletado `)\r\n              } else {\r\n                alert(` erro ao tentar deletar o usuário ${user.username} `)\r\n              }\r\n            }).catch(err => {\r\n              this.ERROS = err\r\n              alert(` erro ao tentar deletar o usuário ${user.username} `)\r\n            })\r\n          }\r\n          tst.push(user)\r\n        })\r\n\r\n        this.filterUser = tst\r\n        this.userTableData = tst\r\n        this.filterUser.sort(function(a,\r\n          b) {\r\n          if (b.isOn.length > a.isOn.length) {\r\n            return -1;\r\n          }\r\n        })\r\n        this.userTableData = this.filterUser\r\n\r\n        //   this.userTableData.sort(this.compare)\r\n      }).catch(err => {\r\n        this.ERROS = err\r\n        //  window.location.replace('/')\r\n      })\r\n    },\r\n    computed: {\r\n      computedTableUser: function () {\r\n        if (!this.filterUser) {\r\n          return []\r\n\r\n        } else {\r\n          const first = (this.paginationUser.page -1) * this.paginationUser.perPage\r\n          const last = this.paginationUser.page * this.paginationUser.perPage\r\n          return this.filterUser.slice(first, last)\r\n        }\r\n      },\r\n      //  computedItens: function () {}\r\n    },\r\n    mounted() {\r\n      setInterval(async () => {\r\n        await req.get(`users`).then(res => {\r\n          this.userTableData = res.data.result\r\n          this.filterUser = res.data.result\r\n\r\n          let tst = []\r\n          this.userTableData.forEach(user => {\r\n\r\n            let vpu = formatRelative(new Date(user.updatedAt), new Date()).toString()\r\n            user.updatedAt = vpu\r\n            if (user.isOn) {\r\n              user.isOn = 'online'\r\n            } else {\r\n              user.isOn = 'offline'\r\n            }\r\n\r\n            let r = formatDistanceToNowStrict(new Date(user.expire), {\r\n              unit: 'second',\r\n              addSuffix: true\r\n            })\r\n            if (r.includes('in')) {\r\n              let secs = r.split(' ')[1].toString()\r\n              let date = new Date(null)\r\n              date.setSeconds(secs)\r\n              date = date.toISOString()\r\n              date = date.toString()\r\n              date = date.split('T')\r\n              let sone = date[0].split('-')\r\n              let stwo = date[1].split(':')\r\n              let d = sone[2]\r\n              let h = stwo[0]\r\n              let m = stwo[1]\r\n              let s = stwo[2].slice(0, 2)\r\n              let z = `${d-1}d:${h}h:${m}m:${s}s`\r\n              user.expire = z\r\n            } else {\r\n              req.delete('users/'+user._id).then(del => {\r\n                if (del.data.success) {\r\n                  alert(`usuário ${user.username} Deletado `)\r\n                } else {\r\n                  alert(` erro ao tentar deletar o usuário ${user.username} `)\r\n                }\r\n              }).catch(err => {\r\n                this.ERROS = err\r\n                alert(` erro ao tentar deletar o usuário ${user.username} `)\r\n              })\r\n            }\r\n            tst.push(user)\r\n\r\n            this.filterUser = tst\r\n            this.userTableData = tst\r\n            this.filterUser.sort(function(a, b) {\r\n              if (b.isOn.length > a.isOn.length) {\r\n                return -1;\r\n              }\r\n            })\r\n            this.userTableData = this.filterUser\r\n          }).catch(err => {\r\n            this.ERROS = err\r\n          })\r\n        })\r\n        this.userTableData.sort(function (a, b) {\r\n          return b.isOn.length - a.isOn.length\r\n        });\r\n      }, 10000)\r\n    }\r\n  }\r\n  </script>\r\n  <style scoped>\r\n@import url('https://fonts.googleapis.com/css2?family=DM+Sans:wght@500&display=swap');\r\n    * {\r\n      outline: none;\r\n    }\r\n\r\n    .container {\r\n      min-height: 100vh;\r\n      height: -webkit-fill-available;\r\n      min-width: 100vw;\r\n      background-color: var(--back-color);\r\n    }\r\n    .noutil {\r\n      min-height: 75px;\r\n    }\r\n    h1 {\r\n      font-size: 2rem;\r\n      font-family: 'DM Sans', sans-serif;\r\n      color: var(--text-full);\r\n      letter-spacing: 1.5px;\r\n      padding: 10px;\r\n    }\r\n    .container-2 {\r\n      display: flex;\r\n      justify-content: center;\r\n      align-items: center;\r\n      width: 80%;\r\n      flex-direction: column;\r\n      max-width: 800px;\r\n      margin: 0 auto;\r\n    }\r\n    .container-2 > div {\r\n      flex: 1;\r\n      background-color: var(--back-color-medium);\r\n      color: var(--text-full);\r\n      margin-top: 5%;\r\n      justify-content: center;\r\n      align-items: center;\r\n      margin-bottom: 5%;\r\n      width: 95%;\r\n\r\n    }\r\n    label {\r\n      color: var(--text-full);\r\n      font-weight: 400;\r\n      margin: 2% auto;\r\n      letter-spacing: 1px;\r\n      font-size: 1rem;\r\n      text-align: left;\r\n      margin-left: 5%;\r\n    }\r\n    button {\r\n      width: 40%;\r\n      margin: 2% auto;\r\n      max-width: 200px;\r\n      background-color: transparent;\r\n      color: var(--text-full);\r\n      font-weight: 500;\r\n      border-radius: 7px;\r\n      font-size: 1rem;\r\n      border: solid 1px var(--text-full);\r\n      height: 40px;\r\n      letter-spacing: 1px;\r\n      font-family: verdana;\r\n      margin-bottom: 10%;\r\n\r\n    }\r\n    button:hover {\r\n      font-size: 1.1rem;\r\n      transition: all.3s;\r\n      background-color: rgb(0,0,0),0.2;\r\n      outline: none;\r\n      box-shadow: 0 0 5px #fff;\r\n    }\r\n    button:active {\r\n      background-color: rgb(255,255,255,0.3);\r\n      transition: background-color .1s;\r\n      height: 35px;\r\n      width: 39%;\r\n    }\r\n    .card-footer {\r\n      border: none;\r\n    }\r\n    input {\r\n      background-color: var(--back-color);\r\n      border: none;\r\n      height: 40px;\r\n      border-radius: 5px;\r\n      color: var(--text-full);\r\n      font-size: 1rem;\r\n      margin: 2% auto;\r\n      width: 90%;\r\n    }\r\n    .multiselect {\r\n      background-color: var(--back-color);\r\n      color: var(--text-full);\r\n      border-radius: 5px;\r\n      text-align: center;\r\n      width: 60%;\r\n      margin: 2% auto;\r\n      outline: none;\r\n    }\r\n@media(min-width:768px) {}\r\n\r\n  </style>"]}]}