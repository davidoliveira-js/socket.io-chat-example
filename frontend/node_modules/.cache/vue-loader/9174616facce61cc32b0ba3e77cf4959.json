y-content: space-between;\r\n      background-color: var(--back-color-medium);\r\n      height: 20%;\r\n      min-height: 80px;\r\n      max-height: 100px;\r\n      min-width: 100%;\r\n      border-bottom: solid 2px rgb(0,0,0,.3);\r\n    }\r\n    .sidebar-header-icon {\r\n      margin-left: 5%;\r\n      font-size: 3rem;\r\n      color: var(--text-full);\r\n    }\r\n    .sidebar-header-text {\r\n      text-align: center;\r\n      margin-right: 50%;\r\n      font-size: 1.2rem;\r\n      color: var(--text-full)\r\n    }\r\n    .sidebar-body {\r\n      flex: 1;\r\n      display: flex;\r\n      align-content: center;\r\n      justify-content: center;\r\n      flex-direction: column;\r\n      margin-top: 20%;\r\n      width: 100%;\r\n    }\r\n    .sidebar-body-section {\r\n      flex: 1;\r\n      display: flex;\r\n      align-items: left;\r\n      flex-direction: column;\r\n      margin-bottom: 5%;\r\n    }\r\n    .sidebar-body-title {\r\n      margin: 0;\r\n      margin-right: 95%;\r\n      margin-left: 2%;\r\n      flex-wrap: nowrap;\r\n      font-size: 1rem;\r\n      color: var(--text-full);\r\n      font-weight: 500;\r\n    }\r\n    .sidebar-body-group {\r\n      min-width: 100%;\r\n      min-height: 100%;\r\n      margin-right: 90%;\r\n      margin: 5% auto;\r\n    }\r\n    .sidebar-body-item {\r\n      width: 100%;\r\n    }\r\n    .icon-left {\r\n      float: left;\r\n      margin-left: 3%;\r\n      color: var(--text-full);\r\n    }\r\n    .body-item-text {\r\n      float: left;\r\n      margin-left: 3%;\r\n      color: var(--text-color);\r\n    }\r\n    .icon-rigth {\r\n      float: right;\r\n      margin-right: 2%;\r\n      color: var(--text-full);\r\n    }\r\n\r\n    @media(min-width:768px){\r\n      .sidebar{\r\n        width: 20%;\r\n      }\r\n      .overlay{\r\n        width: 70%;\r\n        background-color: transparent;\r\n      }\r\n\r\n      \r\n    }\r\n\r\n</style>"]}]}