\t\t\t\t\t\t<div style=\"color:var(--text-full)\" class=\"card-header-title\">\n\t\t\t\t\t\t\tCadastro de usuários\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"card-content\">\n\t\t\t\t\t\t<div class=\"container-form\">\n\t\t\t\t\t\t\t<label class=\"label\"> Usuário</label>\n\t\t\t\t\t\t\t<input class=\"input is-small\" type=\"text\" v-model=\"username\" />\n\t\t\t\t\t\t\t<label class=\"label\">Senha</label>\n\t\t\t\t\t\t\t<input class=\"input is-small\" type=\"password\" v-model=\"password\" />\n\t\t\t\t\t\t\t<label class=\"label\"> Data de expiração </label>\n\t\t\t\t\t\t\t<select class=\"select\" id=\"selectEx\" v-model=\"expiration\">\n\t\t\t\t\t\t\t\t<option value=\"0\">1 dia </option>\n\t\t\t\t\t\t\t\t<option value=\"1\"> 2 dia </option>\n\t\t\t\t\t\t\t\t<option value=\"2\"> 3 dia </option>\n\t\t\t\t\t\t\t</select>\n\t\t\t\t\t\t\t<label class=\"label\"> Checker </label>\n\t\t\t\t\t\t\t<select class=\"select\" id=\"selectChecker\" v-model=\"checker\">\n\t\t\t\t\t\t\t\t<option v-for=\"chkr in checkers\" :key=\"chkr._id\" :value=\"chkr._id\"> {{chkr.slug}} </option>\n\t\t\t\t\t\t\t</select>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"card-footer\">\n\t\t\t\t\t\t<button @click=\"Register\" class=\"button is-success\"> Cadastrar </button>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"card\">\n\t\t\t\t\t<div class=\"card-header\">\n\t\t\t\t\t\t<div style=\"color:var(--text-full)\" class=\"card-header-title\">\n\t\t\t\t\t\t\tCadastro de Checker\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"card-content\">\n\t\t\t\t\t\t<div style=\"color:var(--text-full)\" class=\"container-form\">\n\t\t\t\t\t\t\t<label class=\"label\"> Nome </label>\n\t\t\t\t\t\t\t<input class=\"input is-small\" type=\"text\" v-model=\"chk.name\" />\n\t\t\t\t\t\t\t<label class=\"label\"> Slug </label>\n\t\t\t\t\t\t\t<input class=\"input is-small\" type=\"text\" v-model=\"chk.slug\" />\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"card-footer\">\n\t\t\t\t\t\t<button @click=\"RegisterChk\" class=\"button is-success\"> Cadastrar </button>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</template>\n<script>\n\t//\timport create from '@/services/controllers/UserController'\n\timport axios from 'axios'\n\texport default {\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\tusername: '',\n\t\t\t\tpassword: '',\n\t\t\t\texpiration: '',\n\t\t\t\tchecker: '',\n\t\t\t\tid: 0,\n\t\t\t\tchk: {\n\t\t\t\t\tname: '',\n\t\t\t\t\tslug: ''\n\t\t\t\t},\n\t\t\t\tmsgs: {\n\t\t\t\t\tSuccessUser: false,\n\t\t\t\t\tSuccessChk: false,\n\t\t\t\t\tSuccessUchk: false,\n\t\t\t\t\tccampNull: false\n\t\t\t\t},\n\t\t\t\tERROS: [],\n\t\t\t\tcheckers: []\n\t\t\t}\n\t\t},\n\t\tmethods: {\n\t\t\tasync Register() {\n\t\t\t\tlet name = this.username\n\t\t\t\tlet pass = this.password\n\t\t\t\tlet exp = this.expiration\n\t\t\t\tlet chk = this.checker\n\n\t\t\t\tname = name.replace(/ /g, '')\n\t\t\t\tpass = pass.replace(/ /g, '')\n\n\t\t\t\tif (!name && !pass && !exp && !chk) {\n\t\t\t\t\talert('preencha todos os campos')\n\t\t\t\t} else {\n\t\t\t\t\tswitch (exp) {\n\t\t\t\t\t\tcase '0':\n\t\t\t\t\t\t\tvar date = new Date()\n\t\t\t\t\t\t\tvar Exp = date.setDate(date.getDate() +1)\n\t\t\t\t\t\t\tbreak;\n\t\t\t\t\t\tcase '1':\n\t\t\t\t\t\t\tdate = new Date()\n\t\t\t\t\t\t\tExp = date.setDate(date.getDate() +2)\n\t\t\t\t\t\t\tbreak;\n\t\t\t\t\t\tcase '2':\n\t\t\t\t\t\t\tdate = new Date()\n\t\t\t\t\t\t\tExp = date.setDate(date.getDate() +3)\n\t\t\t\t\t\t\tbreak;\n\t\t\t\t\t\tdefault:\n\t\t\t\t\t\t\tdate = new Date()\n\t\t\t\t\t\t\tExp = date.setDate(date.getDate() +1)\n\t\t\t\t\t\t\tbreak;\n\t\t\t\t\t}\n\n\t\t\t\t\taxios.post('http://localhost:7001/users', {\n\t\t\t\t\t\tusername: name,\n\t\t\t\t\t\tpassword: pass,\n\t\t\t\t\t\texpire: Exp,\n\t\t\t\t\t\tchecker: chk,\n\t\t\t\t\t}).then(res => {\n\t\t\t\t\t\tconsole.log(res.data)\n\t\t\t\t\t\tif (res.data.success) {\n\t\t\t\t\t\t\talert('Usuário cadastrado')\n\t\t\t\t\t\t} else {\n\t\t\t\t\t\t\talert(' fail')\n\t\t\t\t\t\t}\n\t\t\t\t\t}).catch(err => {\n\t\t\t\t\t\tthis.ERROS = err\n\t\t\t\t\t\talert('erro')\n\t\t\t\t\t})\n\t\t\t\t}\n\n\t\t\t},\n\t\t\tasync RegisterChk() {\n\t\t\t\tlet name = this.chk.name\n\t\t\t\tlet slug = this.chk.slug\n\n\t\t\t\tname = name.replace(/ /g,\n\t\t\t\t\t'')\n\n\t\t\t\tif (!name || !slug) {\n\t\t\t\t\talert('preencha todos os campos')\n\t\t\t\t} else {\n\t\t\t\t\taxios.post('http://localhost:7001/checkers', {\n\t\t\t\t\t\tname: name,\n\t\t\t\t\t\tslug: slug,\n\t\t\t\t\t}).then(res => {\n\t\t\t\t\t\tif (res.data.success) {\n\t\t\t\t\t\t\talert('checker cadastrado')\n\t\t\t\t\t\t} else {\n\t\t\t\t\t\t\talert('fail')\n\t\t\t\t\t\t}\n\t\t\t\t\t}).catch(err => {\n\t\t\t\t\t\tthis.ERROS = err\n\t\t\t\t\t\talert('erro')\n\t\t\t\t\t})\n\t\t\t\t}\n\n\t\t\t},\n\t\t},\n\t\tasync created() {\n\t\t\tthis.id = this.$route.params.tk\n\n\t\t\tawait axios.get(`http://localhost:7001/users/${this.id}`).then(res => {\n\t\t\t\tlet {\n\t\t\t\t\trole\n\t\t\t\t} = res.data.response\n\n\t\t\t\tif (role != 1) {\n\t\t\t\t\twindow.location.replace('/')\n\t\t\t\t}\n\n\t\t\t}).catch(err => {\n\t\t\t\tthis.ERROS = err\n\t\t\t\twindow.location.replace('/')\n\t\t\t})\n\t\t\tawait axios.get(`http://localhost:7001/checkers/`).then(res => {\n\t\t\t\tthis.checkers = res.data.info\n\t\t\t\talert(this.checkers)\n\t\t\t}).catch(err => {\n\t\t\t\tthis.ERROS = err\n\t\t\t\twindow.location.replace('/')\n\t\t\t})\n\t\t},\n\t}\n</script>\n<style scoped>\n\n\t.container{\n\t\theight: 100%;\n\t\twidth: 100%;\n\t\tbackground-color: var(--back-color);\n\t}\n\t.container-2{\n\t\tdisplay: flex;\n\t\tjustify-content: center;\n\t\talign-items: center;\n\t\twidth: 80%;\n\t\tflex-wrap: wrap;\n\t\tmax-width: 800px;\n\t\tmargin: 0 auto;\n\t}\n\t.container-2 > div {\n\t\tflex: 1 1 200px;\n\t\tbackground-color: var(--back-color-medium);\n\t\tcolor: var(--text-full);\n\t\tmargin-top: 10%;\n\t}\n\tlabel{\n\t\tcolor: var(--text-full);\n\t}\n\tbutton{\n\t\twidth: 40%;\n\t\tmargin: 2% auto;\n\t}\n\t.card-footer{\n\t\tborder: none;\n\t}\n\t\n</style>"]}]}